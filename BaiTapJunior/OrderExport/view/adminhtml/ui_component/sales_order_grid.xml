<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="export_order">
                <settings>
                    <confirm>
                        <message translate="true">Export Order?</message>
                        <title translate="true">Confirm Export</title>
                    </confirm>
                    <type>export_order</type>
                    <url path="export/order/export"/>
                    <label translate="true">Export Order</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
